{
  "name": "pocket",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "lib": "node toolchain lib",
    "gzip": "gzip --best --keep --no-name deploy/dist/index.html",
    "prepare": "rm -rf deploy/dist && mkdir deploy/dist",
    "build": "npm run prepare && NODE_ENV=production node toolchain build",
    "start": "npm run prepare && NODE_ENV=development node toolchain start",
    "deno": "npm run build && deno run -A deploy/index.js"
  },
  "dependencies": {
    "classcat": "^5.0.3",
    "superfine": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@mapbox/rehype-prism": "^0.8.0",
    "@mdx-js/esbuild": "^2.1.1",
    "@tabler/icons": "^1.68.0",
    "babel-plugin-preval": "^5.1.0",
    "clean-css": "^5.3.0",
    "culori": "^2.0.3",
    "esbuild": "^0.14.39",
    "escape-html": "^1.0.3",
    "mini-svg-data-uri": "^1.4.4",
    "sass": "^1.51.0",
    "svgo": "^2.8.0",
    "typescript": "^4.6.4",
    "uglify-js": "^3.15.4"
  },
  "optionalDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0"
  }
}
